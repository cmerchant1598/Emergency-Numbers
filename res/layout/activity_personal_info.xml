<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_personal_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.a611solutions.emergencynumbers.PersonalInfoActivity"
    android:background="@drawable/mapbgtrd">

    <TextView
        android:text="Developer  Info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/infoTitle"
        android:textStyle="normal|bold"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"/>

    <TextView
        android:text="For more information visit our website below"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/infoTitle"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:id="@+id/adrs"
        android:textColor="@android:color/background_light"
        android:textStyle="normal|bold"
        android:textAllCaps="true"/>

    <TextView
        android:text="Emergency Numbers was built by 611 Solutions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:textColor="@android:color/background_light"
        android:textAllCaps="true"
        android:textStyle="normal|bold"
        android:layout_below="@+id/infoTitle"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:text="www.611solutions.com"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/adrs"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView5"
        android:textColor="@android:color/background_light"/>

    <WebView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:id="@+id/webView"/>

</RelativeLayout>
